generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Staking {
  id        String   @id @default(cuid())
  address   String   @unique
  chain     String
  apy       Float
  tvl       Float
  logo      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
